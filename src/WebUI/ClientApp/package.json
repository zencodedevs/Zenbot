  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200 --open",
    "start:dev": "echo 'Starting (Dev)...' && ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "build:ssr": "ng run ZenAchitecture.WebUI:server:dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-prod-no-cache-with-version": "node ./replace.build.js && ng build  --prod && node ./versioning.build.js",
